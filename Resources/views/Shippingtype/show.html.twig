{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Tipo de Envío</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
 {#           <tr>
                <th>Creationdate</th>
                <td>{{ entity.creationdate|date('Y-m-d H:i:s') }}</td>
            </tr>
#}
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('shippingtype') }}">
            Regresar a la lista de Tipos de Envío
        </a>
    </li>
    <li>
        <a href="{{ path('shippingtype_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
