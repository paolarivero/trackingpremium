{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Tax</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Impuesto de agencia master</th>
                <td>{{ entity.mastertax }}</td>
            </tr>
            <tr>
                <th>Impuesto de agencia</th>
                <td>{{ entity.agencytax }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('tax') }}">
            Regresar a la lista de impuestos de agencia
        </a>
    </li>
    <li>
        <a href="{{ path('tax_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
